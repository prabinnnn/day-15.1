let circle = {
  radius: Number(prompt("Enter the radius")),
  areaofcircle: function () {
    let area = Math.PI * Math.pow(this.radius, 2);
    return area;
  },
  perimeterofcircle: function () {
    let perimeter = 2 * Math.PI * this.radius;
    return perimeter;
  },
};
console.log("Area of the circle:", circle.areaofcircle());
console.log("Perimeter of the circle:", circle.perimeterofcircle());
